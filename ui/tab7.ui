   <property name="styleSheet">
    <string notr="true">/* ===== 浅色主题基础 ===== */
QWidget#centralwidget { background:#f5f6f8; color:#333; font-family:&quot;Microsoft YaHei UI&quot;; }

/* 卡片容器 */
QFrame#status_frame, QFrame#graphic_frame { background:#ffffff; border:1px solid #dcdfe3; border-radius:10px; }

/* 文本组件 */
QTextBrowser, QPlainTextEdit { background:#ffffff; border:1px solid #d2d5da; border-radius:6px; padding:4px; font-size:10pt; }
QTextBrowser .warn { color:#d48806; }
QTextBrowser .error { color:#d32029; }

/* 标题/标签 */
QLabel#label, QLabel#video_slider_text { font-weight:600; letter-spacing:0.5px; color:#555; }

/* 按钮 */
QPushButton { background:#2F6FEB; border:1px solid #2a62d6; border-radius:6px; color:#fff; padding:5px 16px; font-size:10pt; }
QPushButton:hover { background:#4785ff; }
QPushButton:pressed { background:#1d4fa8; }
QPushButton:disabled { background:#c5ccd4; color:#ffffff; }

/* 滑块 */
QSlider::groove:horizontal { height:6px; background:#dcdfe3; border-radius:3px; }
QSlider::handle:horizontal { background:#2F6FEB; width:16px; height:16px; margin:-5px 0; border-radius:8px; border:1px solid #2a62d6; }
QSlider::handle:horizontal:hover { background:#4785ff; }

/* 分隔线 */
QFrame#line { border:none; background:#dcdfe3; max-height:1px; }

/* 状态栏 */
QStatusBar { background:#ffffff; border-top:1px solid #dcdfe3; }

/* 滚动区域透明背景 */
QScrollArea { border:0; background:transparent; }
QScrollArea QWidget { background:transparent; }
/* Modern Scrollbars */
QScrollBar:vertical, QScrollBar:horizontal { background:transparent; width:10px; height:10px; margin:0; }
QScrollBar::handle { background:#c2c7ce; border-radius:5px; min-height:20px; min-width:20px; }
QScrollBar::handle:hover { background:#aab1ba; }
QScrollBar::handle:pressed { background:#8d949c; }
QScrollBar::add-line, QScrollBar::sub-line { background:transparent; width:0; height:0; }
QScrollBar::add-page, QScrollBar::sub-page { background:transparent; }
